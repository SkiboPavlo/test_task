<div class="form-group">
  <div class="col-md-4">
    <p><strong>Title:</strong>
      <%= @project.title %></p>
    <p><strong>User:</strong>
      <%= @project.user.email %></p>

    <%= link_to "New Tag", new_project_tag_path(@project), class: 'btn btn-primary' %>
    <%= link_to "New Task_list", new_project_task_list_path(@project),
      class: 'btn btn-primary' %>
    <%= link_to 'All Tags', project_tags_path(@project), class: 'btn btn-warning' %>
    <%= link_to 'All Task_lists', project_task_lists_path(@project),
      class: 'btn btn-warning' %>
    <%= link_to 'Back', projects_path, class: 'btn btn-default' %>
  </div>
</div>
