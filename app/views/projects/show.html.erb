<form>
  <div class="form-group">
<p>
  <strong>Title:</strong>
  <%= @project.title %>
</p>

<p>
  <strong>User:</strong>
  <%= @project.user.email %>
</p>
</div>
</form>

<%= link_to "New Tag", new_project_tag_path(@project), class: 'btn btn-default' %>
